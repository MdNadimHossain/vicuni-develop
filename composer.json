        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles",
            "Utilities\\composer\\DrupalSettings::create"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "enable-patching": true,
        "composer-exit-on-patch-failure": true,
        "installer-paths": {
            "docroot": [
                "type:drupal-core"
            ],
            "docroot/profiles/vicuni/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "docroot/profiles/vicuni/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "docroot/profiles/vicuni/libraries/{$name}": [
                "type:drupal-library"
            ],
            "docroot/profiles/vicuni/drush/{$name}": [
                "type:drupal-drush"
            ]
        },
        "preserve-paths": [
            "docroot/profiles/vicuni/vicuni.info",
            "docroot/profiles/vicuni/vicuni.profile",
            "docroot/profiles/vicuni/modules/custom",
            "docroot/profiles/vicuni/themes/custom",
            "docroot/profiles/vicuni/static",
            "docroot/sites/default/settings.php",
